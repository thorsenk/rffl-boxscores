# Copy this file to .env and update values as needed
# Usage: source .env

# Public or private league ID (required)
export LEAGUE=323196

# Private leagues only: authentication cookies from espn.com
# Obtain via browser DevTools → Application/Storage → Cookies → espn.com
# ESPN_S2 is a long string; SWID has braces like {XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX}
export ESPN_S2="your_espn_s2_cookie_value"
export SWID="{your_swid_cookie_value}"

